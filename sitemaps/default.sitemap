    }
  }

 	Text label="XmasLights" icon="rgb"{
    Frame label="XmasLights" {
 		  Switch item=FF_GreatRoom_Wled_MasterControls_XmasTree
      Slider item=FF_GreatRoom_Wled_MasterControls_XmasTree
